<?xml version="1.0" encoding="UTF-8"?>

<configuration>
    <appenders>
        <console name="console" target="SYSTEM_OUT">
            <patternLayout pattern="mkproj: %m%n"/>
        </console>
    </appenders>

    <loggers>
        <logger name="zensols.mkproj" level="info"/>
        <root level="warn">
            <appenderRef ref="console"/>
        </root>
    </loggers>
</configuration>
